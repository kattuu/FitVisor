<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="e.vishal.fitvisor">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".DietQuestionsActivity" />
        <activity android:name=".BmiActivity" />
        <activity android:name=".WorkoutPlanActivity" />
        <activity android:name=".DietPlanActivity" />
        <activity
            android:name=".Extras"
            android:label="@string/title_activity_extras"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MedicalCondition" />
        <activity android:name=".ThingsCarry" />
        <activity android:name=".Type2Diabetes" />
        <activity android:name=".HighCholesterol" />
        <activity android:name=".HighBloodPressure" />
        <activity android:name=".SmokeActivity" />
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FinalActivity"></activity>
    </application>

</manifest>